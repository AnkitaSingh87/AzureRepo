						}
					],
					"transformations": [
						{
							"name": "selectcolumns"
						},
						{
							"name": "filtercols"
						},
						{
							"name": "split1"
						},
						{
							"name": "Replacenull"
						},
						{
							"name": "Maxproduct"
						},
						{
							"name": "alter"
						}
					],
					"scriptLines": [
						"source(output(",
						"          transaction_id as string,",
						"          transactional_date as timestamp,",
						"          product_id as string,",
						"          customer_id as short,",
						"          payment as string,",
						"          credit_card as string,",
						"          loyalty_card as string,",
						"          cost as string,",
						"          quantity as string,",
						"          price as double",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> Dataflow",
						"Dataflow select(mapColumn(",
						"          transaction_id,",
						"          transactional_date,",
						"          product_id,",
						"          customer_id,",
						"          payment,",
						"          price",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> selectcolumns",
						"selectcolumns filter(customer_id !=12) ~> filtercols",
						"filtercols split(payment=='visa',",
						"     payment=='mastercard',",
						"     disjoint: false) ~> split1@(visa, mastercard, amex)",
						"split1@amex derive(payment = coalesce(payment,'N/A')) ~> Replacenull",
						"split1@visa aggregate(groupBy(customer_id),",
						"     product_id = max(product_id)) ~> Maxproduct",
						"Maxproduct alterRow(insertIf(1==1)) ~> alter",
						"alter sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          transaction_id as string,",
						"          transactional_date as string,",
						"          product_id as string,",
						"          customer_id as string,",
						"          payment as string,",
						"          credit_card as string,",
						"          loyalty_card as string,",
						"          cost as string,",
						"          quantity as string,",
						"          price as string",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> Outputreport"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Dataflow_data')]",
				"[concat(variables('factoryId'), '/datasets/Dataflow_output')]"
			]
