		{
			"name": "[concat(parameters('factoryName'), '/Source_csv_dataflow')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "Dataflow_data",
								"type": "DatasetReference"
							},
							"name": "Dataflow"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "Dataflow_output",
